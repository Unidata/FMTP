CC = g++
INCLUDE = ../
LIB =
ELFFILE = vcmtpSendv3
DEBUG_FLAG = DEBUG

$(ELFFILE): test.cpp UdpSocket.cpp TcpSend.cpp senderMetadata.cpp vcmtpSendv3.cpp
	$(CC) -D$(DEBUG_FLAG) -I$(INCLUDE) -pthread -o $(ELFFILE) test.cpp UdpSocket.cpp TcpSend.cpp senderMetadata.cpp vcmtpSendv3.cpp

.PHONY : clean
clean:
	rm $(ELFFILE)
