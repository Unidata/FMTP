CC = g++
INCLUDE = ../
LIB =
ELFFILE = vcmtpSend
VCMTPBASE = ../
#DEBUG_FLAG = DEBUG

$(ELFFILE): main.cpp vcmtpSend.cpp $(VCMTPBASE)vcmtpBase.cpp
